<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >  
<mapper namespace="com.eebbk.spring.mvc.check.dao.CheckDbMapper" >  
    <insert id="insert" parameterType="com.eebbk.spring.mvc.check.pojo.CheckDb">  
        insert into t_check_db() 
        values()
        <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id"> 
			SELECT LAST_INSERT_ID() 
		</selectKey>
    </insert>  
    
    <select id="select" resultType="com.eebbk.spring.mvc.check.pojo.CheckDb">  
        select * from t_check_db t_check_db where id = #{checkDb.id}
    </select>  
    
    <delete id="delete">  
        delete from t_check_db where id = #{checkDb.id}
    </delete>  
</mapper>  